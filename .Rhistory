n <- length(mondata$IR)
moyenne <- mean(mondata$IR)
ecart_type <- sd(mondata$IR)
se <- ecart_type / sqrt(n)  # Erreur standard
alpha <- 0.05
t_critique <- qt(1 - alpha/2, df=n-1)
IC_lower <- moyenne - t_critique * se
IC_upper <- moyenne + t_critique * se
c(IC_lower, IC_upper)
